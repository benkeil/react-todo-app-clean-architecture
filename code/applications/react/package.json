    "clean": "rm -rf dist/",
    "prebuild": "npm run clean",
    "build": "webpack --mode=production",
    "start": "webpack serve --port 1338 --mode=development",
    "build:svg": "npm run build:svg:icons",
    "prebuild:svg:icons": "svgo -i src/assets/icons --config=.svgo.icons.yml",
    "build:svg:icons": "svg-sprite --config svg.config.json src/assets/icons/*.svg",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test": "jest",
    "preversion": "mkdir .git || true",
    "release:patch": "npm version patch && npm publish && git push origin main --tags",
    "release:minor": "npm version minor && npm publish && git push origin main --tags",
    "release:major": "npm version major && npm publish && git push origin main --tags",
    "generate-sources": "yarn run generate-sources:css",
    "generate-sources:css": "node -r ts-node/register src/build/css.build.ts",
    "watch": "yarn run watch:css",
    "watch:css": "nodemon --config src/build/css.nodemon.json src/build/css.build.ts"
  },
